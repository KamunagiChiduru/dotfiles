use Shougo/vimproc.vim
    as vimproc
    enable_if "!(has('win16') || has('win32') || has('win64') || has('win95'))"
    build {
        on windows
            - mingw32-make -f make_mingw64.mak
        on mac
            - make -f make_mac.mak
        on unix
            - make -f make_unix.mak
    }

